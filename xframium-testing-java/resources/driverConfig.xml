<xFramium xmlns="http://www.xframium.org/xFramiumDriver" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.xframium.org/xFramiumDriver http://www.xframium.org/xsd/xFramiumDriver.xsd">
  <cloud provider="XML" fileName="cloudRegistry.xml" name="saucelabs" />
  <application provider="XML" name="xTest" fileName="applicationRegistry.xml" />
  <devices provider="XML" fileName="deviceRegistry.xml">
  </devices>
  <references>
    <reference fileName="../../testing/driverConfig.xml" tests="true" functions="true" elements="true" applications="false" devices="false" data="true"/>
    <reference fileName="./driverConfigBDD.xml" tests="true" functions="true" elements="true" applications="false" devices="false" data="true"/>
  </references>
  <driver type="WEB" outputFolder="test-output" displayResults="true" suiteName="xTest" embeddedServer="true" testNames="Call Function,Exists Test,Call two methods with data,Call two methods">
    <property name="selenium.timeouts.implicitWait">5000</property>
    <propertyAdapter className="org.xframium.device.property.SeleniumPropertyAdapter" />
    <artifact type="FAILURE_SOURCE" />
    <artifact type="DEVICE_LOG" />
    <artifact type="EXECUTION_RECORD_HTML" />
    <artifact type="EXECUTION_RECORD_JSON" />
    <artifact type="CONSOLE_LOG" />
    <artifact type="GRID_REPORT" />
    <artifact type="TAB_WEBHOME" />
    <artifact type="SAUCE_LABS" />
    <artifact type="REPORTIUM" />
  </driver>
  <model provider="XML" siteName="xTest" fileName="page-home.xml" />
  <suite provider="LOCAL">
    <test name="Call Function" timed="false" threshold="0" active="true" priority="1" severity="1" dataProvider="authData">
      <description>Calls a simple JAVA based function</description>
      <step type="FUNCTION" name="testKeyword" page="WebHomePage"  failureMode="IGNORE" />
      <step type="FUNCTION" name="testKeywordWithData" page="WebHomePage" failureMode="IGNORE">
        <parameter type="STATIC" value="data{authData}" />
      </step>
      <step type="GHERKIN" name="I call method one" page="home" failureMode="IGNORE"/>
      <step type="CALL2" name="kw-exists" page="home" failureMode="IGNORE"/>
      <step type="FUNCTION" name="testKeyword" page="WebHomePage"  failureMode="IGNORE" />
      <step type="FUNCTION" name="testKeywordWithData" page="WebHomePage" failureMode="IGNORE">
        <parameter type="STATIC" value="data{authData}" />
      </step>
      <step type="GHERKIN" name="I call method one" page="home" failureMode="IGNORE"/>
      <step type="CALL2" name="kw-exists" page="home" failureMode="IGNORE"/>
      <step type="FUNCTION" name="testKeyword" page="WebHomePage"  failureMode="IGNORE" />
      <step type="FUNCTION" name="testKeywordWithData" page="WebHomePage" failureMode="IGNORE">
        <parameter type="STATIC" value="data{authData}" />
      </step>
      <step type="GHERKIN" name="I call method one" page="home" failureMode="IGNORE"/>
      <step type="CALL2" name="kw-exists" page="home" failureMode="IGNORE"/>
      <step type="FUNCTION" name="testKeyword" page="WebHomePage"  failureMode="IGNORE" />
      <step type="FUNCTION" name="testKeywordWithData" page="WebHomePage" failureMode="IGNORE">
        <parameter type="STATIC" value="data{authData}" />
      </step>
      <step type="GHERKIN" name="I call method one" page="home" failureMode="IGNORE"/>
      <step type="CALL2" name="kw-exists" page="home" failureMode="IGNORE"/>
      
    </test>
    <packages>
      <package name="org.xframium.examples">Pull from the EXAMPLES packages</package>
    </packages>
  </suite>
  <data provider="XML" fileName="pageData.xml" />
</xFramium>